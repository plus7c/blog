{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable", 
      "esnext"
    ],  
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["components/*"],
      "@/utils/*": ["utils/*"],
      "@/models/*": ["models/*"]
    },
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
, "app/posts/[id]/page.jsx", "app/api/routets", "app/api/posts/[id]/route.js"  ],
  "exclude": [
    "node_modules"
  ]
}
